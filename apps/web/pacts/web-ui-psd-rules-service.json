{
  "consumer": {
    "name": "web-ui"
  },
  "interactions": [
    {
      "description": "a request for the rules summary",
      "request": {
        "headers": {
          "accept": "application/json"
        },
        "method": "GET",
        "path": "/rules/summary"
      },
      "response": {
        "body": {
          "as_of": "2024-10-18T14:10:00Z",
          "counters": {
            "critical": 2,
            "info": 1,
            "total": 5,
            "warning": 2
          },
          "focus_symbols": [
            "TSLA"
          ],
          "top": [
            {
              "description": "Portfolio level VaR exceeded the configured limit.",
              "id": "breach-portfolio-var",
              "occurred_at": "2024-10-18T14:05:00Z",
              "rule": "Portfolio VaR Limit",
              "severity": "critical",
              "subject": "Aggregate VaR",
              "symbol": "SPX"
            }
          ]
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.as_of": {
              "combine": "AND",
              "matchers": [
                {
                  "format": "yyyy-MM-dd'T'HH:mm:ssxxx",
                  "match": "datetime"
                }
              ]
            },
            "$.counters": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.counters.critical": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.counters.info": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.counters.total": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.counters.warning": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.focus_symbols": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            },
            "$.focus_symbols[*]": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.top": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            },
            "$.top[*].description": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.top[*].id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.top[*].occurred_at": {
              "combine": "AND",
              "matchers": [
                {
                  "format": "yyyy-MM-dd'T'HH:mm:ssxxx",
                  "match": "datetime"
                }
              ]
            },
            "$.top[*].rule": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.top[*].severity": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "critical|warning|info"
                }
              ]
            },
            "$.top[*].subject": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.top[*].symbol": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {
            "$['Content-Type'][0]": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json;?.*"
                }
              ]
            }
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "10.4.1"
    },
    "pactRust": {
      "ffi": "0.4.0",
      "models": "1.0.4"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "psd-rules-service"
  }
}